{
    "name": "codecontrol/framework-bundle",
    "type": "symfony-bundle",
    "description": "FrameworkBundle for Symfony2 web apps",
    "keywords": ["symfony","bundle","framework"],
    "homepage": "https://github.com/codecontrol/framework-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Code Control",
            "email": "info@codecontrol.rs",
            "homepage": "http://www.codecontrol.rs/en"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/config" : "~2.5",
        "symfony/yaml" : "~2.5",
        "symfony/dependency-injection" : "~2.5",
        "symfony/event-dispatcher" : "~2.5",
        "symfony/http-kernel": "~2.5",
        "symfony/http-foundation": "~2.5",
        "symfony/validator": "~2.5",
        "symfony/translation": "~2.5",
        "symfony/templating": "~2.5",
        "symfony/twig-bundle": "~2.5"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "0.*"
    }
}
