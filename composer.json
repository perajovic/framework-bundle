{
    "name": "supportyard/framework-bundle",
    "type": "symfony-bundle",
    "description": "FrameworkBundle for Symfony2 web apps",
    "keywords": ["symfony","bundle","framework"],
    "homepage": "https://github.com/supportyard/framework-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "SupportYard",
            "homepage": "https://supportyard.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/config" : "~2.7",
        "symfony/yaml" : "~2.7",
        "symfony/dependency-injection" : "~2.7",
        "symfony/event-dispatcher" : "~2.7",
        "symfony/http-kernel": "~2.7",
        "symfony/http-foundation": "~2.7",
        "symfony/validator": "~2.7",
        "symfony/translation": "~2.7",
        "symfony/templating": "~2.7",
        "symfony/twig-bundle": "~2.7"
    },
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "0.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}
