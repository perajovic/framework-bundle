{
    "name": "filos/framework-bundle",
    "type": "symfony-bundle",
    "description": "FrameworkBundle for building Symfony web apps",
    "keywords": ["symfony","bundle","framework"],
    "homepage": "https://github.com/perajovic/framework-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pera Jovic",
            "email": "perajovic@me.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Filos\\FrameworkBundle\\": "src/Filos/FrameworkBundle" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=7.0",
        "symfony/config" : "~3.1",
        "symfony/yaml" : "~3.1",
        "symfony/dependency-injection" : "~3.1",
        "symfony/event-dispatcher" : "~3.1",
        "symfony/http-kernel": "~3.1",
        "symfony/http-foundation": "~3.1",
        "symfony/validator": "~3.1",
        "symfony/translation": "~3.1",
        "symfony/templating": "~3.1",
        "symfony/twig-bundle": "~3.1",
        "symfony/framework-bundle": "~3.1",
        "ramsey/uuid": "~3.5"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "0.*",
        "phpunit/phpunit": "^5.6",
        "friendsofphp/php-cs-fixer": "^1.0",
        "doctrine/orm": "~2.5"
    },
    "suggest": {
        "doctrine/dbal": "Allow using doctrine custom types",
        "doctrine/orm": "Allow using custom doctrine event listeners"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
